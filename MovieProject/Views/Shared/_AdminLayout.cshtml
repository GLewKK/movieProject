
@{
    ViewBag.Title = "_AdminLayout";
}

<h2>_AdminLayout</h2>

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <script src="~/Content/admin/plugins/bower_components/gmaps/gmaps.js"></script>

    <meta name="description" Content="">
    <meta name="author" Content="">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/admin/plugins/images/favicon.png">
    
    <link href="~/Content/admin/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <title>Ample Admin Template - The Ultimate Multipurpose admin template</title>
    <!-- Menu CSS -->
    <link href="~/Content/admin/plugins/bower_components/sidebar-nav/dist/sidebar-nav.min.css" rel="stylesheet">
    <!-- toast CSS -->
    <link href="~/Content/admin/plugins/bower_components/toast-master/css/jquery.toast.css" rel="stylesheet">
    <!-- morris CSS -->
    <link href="~/Content/admin/plugins/bower_components/morrisjs/morris.css" rel="stylesheet">
    <!-- chartist CSS -->
    <link href="~/Content/admin/plugins/bower_components/chartist-js/dist/chartist.min.css" rel="stylesheet">
    <link href="~/Content/admin/plugins/bower_components/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.css" rel="stylesheet">
    <!-- animation CSS -->
    <link href="~/Content/admin/css/animate.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/Content/admin/css/style.css" rel="stylesheet">
    <!-- color CSS -->
    <link href="~/Content/admin/css/colors/default.css" id="theme" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body class="fix-header">
    <!-- ============================================================== -->
    <!-- Preloader -->
    <!-- ============================================================== -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>
    <!-- ============================================================== -->
    <!-- Wrapper -->
    <!-- ============================================================== -->
    <div id="wrapper">
        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        <nav class="navbar navbar-default navbar-static-top m-b-0">
            <div class="navbar-header">
                <div class="top-left-part">
                    <!-- Logo -->
                    <a class="logo" href="#">
                        <!-- Logo icon image, you can use font-icon also --><b>
                            <!--This is dark logo icon--><img src="~/Content/admin/plugins/images/admin-logo.png" alt="home" class="dark-logo" /><!--This is light logo icon--><img src="~/Content/admin/plugins/images/admin-logo-dark.png" alt="home" class="light-logo" />
                        </b>
                        <!-- Logo text image you can use text also --><span class="hidden-xs">
                            <!--This is dark logo text--><img src="~/Content/admin/plugins/images/admin-text.png" alt="home" class="dark-logo" /><!--This is light logo text--><img src="~/Content/admin/plugins/images/admin-text-dark.png" alt="home" class="light-logo" />
                        </span>
                    </a>
                </div>
                <!-- /Logo -->
                <ul class="nav navbar-top-links navbar-right pull-right">
                    
                    
                </ul>
            </div>
            <!-- /.navbar-header -->
            <!-- /.navbar-top-links -->
            <!-- /.navbar-static-side -->
        </nav>
        <!-- End Top Navigation -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <div class="navbar-default sidebar" role="navigation">
            <div class="sidebar-nav slimscrollsidebar">
                <div class="sidebar-head">
                    <h3><span class="fa-fw open-close"><i class="ti-close ti-menu"></i></span> <span class="hide-menu">Navigation</span></h3>
                </div>
                <ul class="nav" id="side-menu">
                    <li style="padding: 70px 0 0;">
                        <a href="@Url.Action("Index","Home")" class="waves-effect"><i class="fa fa-home fa-fw" aria-hidden="true"></i>Home</a>
                    </li>
                    <li>
                        <a href="@Url.Action("CreateGenre","Home")" class="waves-effect"><i class="fa fa-font fa-fw" aria-hidden="true"></i>Genre</a>
                        
                    </li>
                    <li>
                        <a href="@Url.Action("Index","Actor")" class="waves-effect"><i class="fa fa-user fa-fw" aria-hidden="true"></i>Actor</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index","Producer")" class="waves-effect"><i class="fa fa-star fa-fw" aria-hidden="true"></i>Producer</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index","Auditorium")" class="waves-effect"><i class="fa fa-group fa-fw" aria-hidden="true"></i>Auditorium</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Create","Movie")" class="waves-effect"><i class="fa fa-video-camera fa-fw" aria-hidden="true"></i>Movie</a>
                    </li>


                </ul>
            </div>

        </div>
        <!-- ============================================================== -->
        <!-- End Left Sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page Content -->
        <!-- ============================================================== -->
        @RenderBody()
        <!-- ============================================================== -->
        <!-- End Page Content -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script src="~/Content/admin/plugins/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Content/admin/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Menu Plugin JavaScript -->
    <script src="~/Content/admin/plugins/bower_components/sidebar-nav/dist/sidebar-nav.min.js"></script>
    <!--slimscroll JavaScript -->
    <script src="~/Content/admin/js/jquery.slimscroll.js"></script>
    <!--Wave Effects -->
    <script src="~/Content/admin/js/waves.js"></script>
    <!--Counter js -->
    <script src="~/Content/admin/plugins/bower_components/waypoints/lib/jquery.waypoints.js"></script>
    <script src="~/Content/admin/plugins/bower_components/counterup/jquery.counterup.min.js"></script>
    <!-- chartist chart -->
    <script src="~/Content/admin/plugins/bower_components/chartist-js/dist/chartist.min.js"></script>
    <script src="~/Content/admin/plugins/bower_components/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.min.js"></script>
    <!-- Sparkline chart JavaScript -->
    <script src="~/Content/admin/plugins/bower_components/jquery-sparkline/jquery.sparkline.min.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/Content/admin/js/custom.min.js"></script>
    <script src="~/Content/admin/js/dashboard1.js"></script>
    <script src="~/Content/admin/plugins/bower_components/toast-master/js/jquery.toast.js"></script>
</body>

</html>
